<config>

<definition>
    <element name="History" edit="false">
        <display class="HiddenRowToggleWdg">
            <dynamic>pyasm.flash.widget.FlashAssetHistoryWdg</dynamic>
        </display>
    </element>
    <element name="code"/>
    <element name="snapshot"/>
    <element name="description"/>
    <element name="search_type"/>
    <!--<element name="category"/>-->
    <element name="project_code"/>
    <element name="update"/>
   
</definition>


<table>
    <element name="History"/>
    <element name="code"/>
    <element name="snapshot"/>
    <element name="description"/>
    <element name="search_type"/>
    <!--<element name="category"/>-->
    <element name="project_code"/>
    <element name="update"/>
   
</table>


<edit>
    
    <element name="project_code">
        <display class="SelectWdg">
            <query>sthpw/project|code|code</query>
            <persist>true</persist>
        </display>
    </element>    
    <element name="description"/>
    <element name="search_type">
   
        <display class="SelectWdg">
            <query>sthpw/search_object|search_type|search_type</query>
            <persist>true</persist>
        </display>
    </element>
    <!--<element name="category"/>-->
    
    <!-- the order matters here, code is autogenerated and should be the last 
         item -->
    <element name="code">
        <display class="NoneWdg"/>
        <action class="pyasm.prod.command.TemplateCodeUpdate"/>
    </element>
</edit>


</config>


